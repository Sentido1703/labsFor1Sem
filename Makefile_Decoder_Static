SOURCES=decoder.c
FORLIBRARY=stringutils.o coders.o
LIBRARY=libString.a
TARGET=decoded

all: $(TARGET)

$(TARGET): decoder.o libString.a
	gcc decoder.o -o $(TARGET) -std=c99 -L. -lString

decoder.o: decoder.c
	gcc $< -c -o $@ -std=c99

$(LIBRARY): $(FORLIBRARY)
	ar cr $(LIBRARY) stringutils.o coder.o

stringutils.o: stringutils.c
	gcc $< -c -o $@ -std=c99

Coder.o: coders.c
	gcc $< -c -o $@ -std=c99

clean:
	rm -f *.o *.a $(TARGET)